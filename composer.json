{
	"name" : "gossi/php-code-profiles",
	"description" : "Profiles for code styles",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Thomas Gossmann",
			"homepage" : "https://gos.si"
		}
	],
	"keywords" : [
		"php",
		"formatter",
		"beautifier"
	],
	"support" : {
		"issues" : "https://github.com/gossi/php-code-profiles/issues"
	},
	"autoload" : {
		"psr-4" : {
			"gossi\\code\\profiles\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"gossi\\code\\profiles\\tests\\" : "tests"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"symfony/config" : "~2|~3|^4",
		"symfony/yaml" : "~2|~3|^4"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5.7",
		"sami/sami" : "~3|~4"
	},
	"scripts" : {
		"test" : "phpunit",
		"api" : "sami.php update sami.php"
	}
}